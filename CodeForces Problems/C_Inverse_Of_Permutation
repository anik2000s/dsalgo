#include <bits/stdc++.h>

using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    int n;
    cin>>n;
    unordered_map<int,int>m;
    for(int i=0;i<n;i++){
        int a;
        cin>>a;
        m.insert({a,i+1});
    }
    for(int i=1;i<=n;i++){
        cout<<m[i]<<" ";
    }
    return 0;
}